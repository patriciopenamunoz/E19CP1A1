<%= link_to 'Add new project', new_project_path, class: 'btn btn-success' %>
<br><br>
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @projects.each do |project| %>
    <tr>
      <td><%= project.name %></td>
      <td><%= link_to 'Details', project_path(project.id), class: 'btn btn-primary' %></td>
    </tr>
    <% end %>
  </tbody>
</table>
